{
  "name": "d2rspeedrun",
  "author": "Kamil Tunkiewicz <kamil.tunkiewicz(at)gmail.com>",
  "version": "1.8.0",
  "description": "Scan your Diablo 2 Resurrected saves and displays stats about the last updated one",
  "main": "./.webpack/main",
  "scripts": {
    "start": "ELECTRON_ENV=development electron-forge start",
    "start-win": "set ELECTRON_ENV=development&&electron-forge start",
    "package": "electron-forge package",
    "build": "yarn run package && yarn run build-mac && yarn run build-win && rm -rf out",
    "build-mac": "electron-builder build -m",
    "build-win": "electron-builder build -w",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint . --ext js,ts"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@dschu012/d2s": "^2.0.18",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@mui/styled-engine-sc": "^5.5.2",
    "@types/electron-json-storage": "^4.5.0",
    "@types/express": "^4.17.13",
    "@types/node-fetch": "^2",
    "@types/request": "^2.48.8",
    "animate.css": "^4.1.1",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "electron-json-storage": "^4.5.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.3",
    "get-port": "^6.1.2",
    "html2canvas": "^1.4.1",
    "material-react-toastify": "^1.0.1",
    "node-fetch": "2",
    "node-loader": "^2.0.0",
    "pretty-ms": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-use-precision-timer": "^1.0.7",
    "request": "^2.88.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-loader": "8.2.4",
    "cross-env": "7.0.3",
    "electron": "16.0.9",
    "electron-builder": "^22.14.13",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-standard": "5.0.0",
    "file-loader": "^6.2.0",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.3",
    "wait-on": "6.0.1"
  },
  "build": {
    "appId": "d2rSpeedRun",
    "productName": "d2rSpeedRun",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./assets/icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "mac": {
      "identity": null
    },
    "nsis": {
      "installerIcon": "./assets/install.ico",
      "uninstallerIcon": "./assets/delete.ico",
      "uninstallDisplayName": "d2rSpeedRun",
      "license": "./assets/license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      ".webpack/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "SpeedRun Tool",
        "executableName": "d2rSpeedRun",
        "icon": "assets/icon",
        "extraResource": [
          "assets"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./electron/bridge.ts"
                  }
                },
                {
                  "html": "./src/stream/index.html",
                  "js": "./src/stream/stream.tsx",
                  "name": "stream"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
